{
  "description": "Bump dprint official plugins",
  "regexManagers": [
    {
      "fileMatch": ["dprint.json"],
      "matchStrings": ["\"https://plugins.dprint.dev/(?<depName>.+)-(?<currentValue>.+?).wasm\""],
      "depNameTemplate": "dprint/dprint-plugin-{{{depName}}}",
      "datasourceTemplate": "github-releases"
    }
  ]
}
